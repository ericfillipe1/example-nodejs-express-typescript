{
  "name": "examplo-nodejs-express-typescipt",
  "private": true,
  "version": "1.0.0",
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  },
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec ts-node src/server-listen.ts",
    "start": "ts-node src/server-listen.ts",
    "lint": "eslint --cache \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write src/**/*.ts",
    "build": "tsc"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "express": "^4.18.2",
    "mongoose": "^7.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
